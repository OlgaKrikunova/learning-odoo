<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="property_offer_report" model="ir.actions.report">
        <field name="name">Print Property Offers</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-html</field>
        <!--        <field name="report_type">qweb-pdf</field>-->
        <field name="report_name">estate.report_property_offers</field>
        <field name="report_file">estate.report_property_offers</field>
        <field name="print_report_name">'Property Offers - %s' % (object.name or '').replace('/', '')</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>

    </record>

    <record id="estate.report_user_properties" model="ir.actions.report">
        <field name="name">User Property Report</field>
        <field name="model">res.users</field>
        <field name="binding_model_id" ref="base.model_res_users"></field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">estate.property_user_report</field>
        <field name="report_file">estate.property_user_report</field>

    </record>



</odoo>
