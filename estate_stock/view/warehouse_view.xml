<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="warehouse_inherit_form" model="ir.ui.view">
        <field name="model">stock.warehouse</field>
        <field name="inherit_id" ref="stock.view_warehouse"/>
        <field name="arch" type="xml">
        <xpath expr="//button[@name='action_view_all_routes']" position="before">
            <button name="action_create_order_from_warehouse"  type="object"
                    string="Create an order from the warehouse"
                    class="btn-secondary"  icon="fa-shopping-cart"/>
        </xpath>
         </field>
    </record>
</odoo>
